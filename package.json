{
  "name": "@sambirsb/uikit",
  "version": "0.2.5",
  "description": "My own ui library for pet projects",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
